{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../priv/static",
    "strict": true,
    "target": "es2017",
    "jsx": "react-jsx"
  },
  "include": ["js"],
  "exclude": ["/node_modules/**/*"]
}
